import binascii
from gmssl import sm3,func#引诱库函数进行hash
import os
def sm3_hash(data):
    return sm3.sm3_hash(func.bytes_to_list(data))
def birthday_attack():
  while (True):#随机碰撞，找到hash值相等的两个输入，时间复杂度仍然很高
    random1=os.urandom(32)
    random2=os.urandom(32)
    msg1=sm3_hash(random1)
    msg2=sm3_hash(random2)
    if msg1==msg2:
      print("random1 是：",random1)
      print("random2 是：",random2)
      print("msg1 是：",msg1)
      print("msg2 是：",msg2)
      break

birthday_attack()
